package model;

import processing.core.PApplet;

public class enfermas extends personas {

	PApplet app; 
	
	public enfermas( int cant, PApplet app ) {
		super(cant, app);
	}
	
	public void pintar() {
		/*this.r = 255;
		this.g = 0;
		this.b = 0;*/
		app.color(255,0,0);
		app.ellipse(posX, posY, tam, tam);
		
	}


	@Override
	public int compareTo(personas o) {
		
		return 0;
	}

	@Override
	
}
